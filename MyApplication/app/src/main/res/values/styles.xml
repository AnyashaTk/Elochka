<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorButtonNormal">@color/colorButton</item>
    </style>

    <style name="ContrastTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="android:colorBackground">@color/colourContrastPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="android:textColor">@color/colourContrastText</item>
        <item name="colorButtonNormal">@color/colourContrastButton</item>
    </style>

</resources>